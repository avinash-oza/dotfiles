# ls
alias ll='ls -altr'
alias nettop='sudo tcptrack -i enp4s0'

# git
alias gs='git status'
alias gd='git diff'

# directories
alias dotfiles='cd $HOME/git_repos/dotfiles'
alias rsnapshot-du='sudo rsnapshot du'
alias make-ansible-dirs='mkdir -p files handlers meta templates tasks defaults'
alias become_nagios='sudo su -s /bin/bash nagios'
